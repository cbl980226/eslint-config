{
  "name": "@cbl980226/eslint-config",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "format:write": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "test": "yarn test:vue && yarn test:react",
    "type-check": "yarn type-check:vue && yarn type-check:react",
    "test:vue": "eslint ./tests/vue.vue --ext .vue",
    "test:react": "eslint ./tests/react.tsx --ext .tsx",
    "type-check:vue": "vue-tsc --noEmit -p tsconfig.vue.json",
    "type-check:react": "tsc --noEmit -p tsconfig.react.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@nrwl/eslint-plugin-nx": "^15.8.3",
    "@types/node": "^18.14.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "nx": "^15.8.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "@nrwl/eslint-plugin-nx": "*",
    "nx": "*"
  },
  "dependencies": {
    "eslint-plugin-vue": "^9.9.0",
    "vue-eslint-parser": "^9.1.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
